--sysroot="/usr/arm-none-eabi"
-Oz
-g0
-fomit-frame-pointer
-momit-leaf-frame-pointer
-fno-common
-mlittle-endian
-std=gnu99
-Wall
-Wextra
-Wno-main
-Werror=int-to-pointer-cast
-Wno-error=int-conversion
-Wimplicit-fallthrough
-Wvla
-Wundef
-Wshadow
-Wformat=2
-Wformat-security
-Wwrite-strings
-fdata-sections
-ffunction-sections
-funsigned-char
-fshort-enums
-mno-unaligned-access
-fropi
-fno-jump-tables
-nostdlib
-nodefaultlibs
-frwpi
-mthumb
--target=armv6m-none-eabi
-mcpu=cortex-m0plus

